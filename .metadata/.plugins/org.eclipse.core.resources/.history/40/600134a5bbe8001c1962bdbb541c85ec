package clasesObjetos;

public class Gato {
	
	String nombre;
	String raza;
	int edad;
	
	void comer() {
		System.out.println("El animaluco no hace más que zampar!");
	}
	
	void dormir() {
		System.out.println("El animaluco está completamente roque");
	}
	void remolonear() {
		System.out.println("El animaluco ronda avisamente...\n¡Deja de morder el cable!");
	}
	
}