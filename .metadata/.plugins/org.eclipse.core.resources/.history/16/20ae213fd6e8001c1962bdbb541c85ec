package calculator;

public class Main {

	public static void main(String[] args) {
		Calculadora calculadora = new Calculadora();
		
		int resultado = calculadora.suma(10, 20);
		float resultadoF = calculadora.suma(10.5f, 20.5f);		
		double resultadoD = calculadora.suma(10, 20);
		
		System.out.println("Resultado int: " + resultado + "\nResultado Float: " + resultadoF 
						+"\nResultado Double: " + resultadoD);
		
	}

}
